{
  "manifest_version": 2,
  "name": "GPHM Smart Copy",
  "version": "1.0",
  "description": "Copies data for spreadsheets",
  "icons": {
    "48": "icons/g48.png"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions": [
    "activeTab",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite"
    ]
}
